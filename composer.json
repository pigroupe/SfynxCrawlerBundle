  "keywords": ["crawler", "Helper", "validator", "xsd", "xml"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "MIT",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxCrawlerBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.3|~3.0"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\CrawlerBundle": "" }
  },

  "target-dir": "Sfynx/CrawlerBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.3-dev"
    }
  }
